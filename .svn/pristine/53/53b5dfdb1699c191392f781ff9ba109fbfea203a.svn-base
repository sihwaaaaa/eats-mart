package vo;

import java.util.Calendar;

public class Buy {
	/**
	 * @author 이시화
	 * 변수선언 및 기본생성자, 게터세터, tostring, using 필드 생성
	 * 
	 */
	private int no; //주문번호
	private int sales; //매출
	private Calendar calendar;//주문시각
	
	public Buy() {
		// TODO Auto-generated constructor stub
	}

	public Buy(int no, int sales, Calendar calendar) {
		super();
		this.no = no;
		this.sales = sales;
		this.calendar = calendar;
	}

	public int getNo() {
		return no;
	}

	public void setNo(int no) {
		this.no = no;
	}

	public int getSales() {
		return sales;
	}

	public void setSales(int sales) {
		this.sales = sales;
	}

	public Calendar getCalendar() {
		return calendar;
	}

	public void setCalendar(Calendar calendar) {
		this.calendar = calendar;
	}

	@Override
	public String toString() {
		return "Buy [no=" + no + ", sales=" + sales + ", calendar=" + calendar + "]";
	}
	
	
}
