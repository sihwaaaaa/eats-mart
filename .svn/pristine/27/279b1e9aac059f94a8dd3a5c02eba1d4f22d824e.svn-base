package vo;

public class Product {

	/**
	 * @author 이창용 필드 선언 및 기본 생성자, 투스트링, 게터세터, 유징필드 생성
	 */
	private int no; // 상품번호
	private String name; // 상품명
	private int price; // 판매가격
	private int cnt; // 재고

	public Product(int no, String name) { // 상위 카테고리 파라미터 설정 "번호", "상품명"
		super();
		this.no = no;
		this.name = name;
	}

	public Product(int no, String name, int price, int cnt) {
		super();
		this.no = no;
		this.name = name;
		this.price = price;
		this.cnt = cnt;
	}

	public Product() {

	}

	@Override
	public String toString() {
		return "Product [no=" + no + ", name=" + name + ", price=" + price + ", cnt=" + cnt + "]";
	}

	public int getNo() {
		return no;
	}

	public void setNo(int no) {
		this.no = no;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public int getCnt() {
		return cnt;
	}

	public void setCnt(int cnt) {
		this.cnt = cnt;
	}

}
